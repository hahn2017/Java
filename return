//return end the execution of method 
public class GetValueLocation
{
    public static void main(String args[])
    {
        int[] ar = {4, 3, 8, 9, 6};
        int value = 3;
        int K = getValueLocation(ar, value);
        System.out.println(K);
    }

    public static int getValueLocation(int[] ar, int value)
    {
        for (int i = 0; i < ar.length; i++)
        {
            if (value == ar[i])
            {
                return i;
            }
        }
        return -1;
    }
}
